setenv LANG ja_JP.UTF-8
escape ^ q
defscrollback 8195
bind s split
bind f eval "encoding UTF-8"
term screen
caption always "%{=r dd}%-Lw%40L>%?%F%{=b dR}%:[%? %n%f %t %?%F%{-}%:]%?%+Lw %?%F%-024=GNU Screen of Git master%:%=%?"
sorendition "+rb .G"
vbell off
bell_msg 'Bell in window %n'

autodetach off
