setenv LANG ja_JP.UTF-8
escape ^ q
defscrollback 8195
bind s split
bind f eval "encoding UTF-8"
bind ^p next
bind ^n prev 
term xterm-256color
caption always "%{=r dd}%-Lw%?%F%{= dm}%:[%? %n%f %t %?%F%{-}%:]%?%+Lw %?%F"
sorendition "= dm"
vbell off
bell_msg 'Bell in window %n'

autodetach off


defbce "on"
term xterm-16color


screen -t Code   0 zsh 
screen -t Base   1 zsh 
screen -t App    2 zsh 
screen -t Schema 3 zsh 
screen -t Kamiya 4 zsh 
screen -t Log1   5 zsh 
screen -t Log2   6 zsh 

#termcapinfo xterm* 'Co#256:pa#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm:'
#attrcolor b ".I"
#defbce "on"
